#include<stdio.h>
#include<math.h>
void main(){
double x=2.05 ,y , a , s=0;
int k=0;
y = cosh(sqrt(x));
printf("y=cosh(sqrt(%.2f))=%.2f \n" ,x,y) ;
a = 1;
s = s + a;
printf("\n \n");
printf("    x \t            a \t            s \n" ,x,a,s) ;
printf("%f \t %f \t %f                k = %d \n" ,x,a,s,k) ;
k++;
while(k!=501){
a = (x*a)/((2*k)*((2*k)-1));
s = s + a;
if (k<3){
printf("%f \t %f \t %f                k = %d \n" ,x,a,s,k) ;
}
k++;
}
//printf("%.2f \t %f \t %f \n" ,x,a,s) ;
}
